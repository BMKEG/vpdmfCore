<ViewDefinition Name="Datum" Type="Data">
	<Primitives>
		<PrimitiveDefinition Name="Datum" primaryFlag="TRUE">
			<Classes>
				<Class Name="Datum" primaryFlag="TRUE"/>
				<Class Name="DataDefinition"/>
			</Classes>
			<triplets>
				<triplet>
					Datum->def=Datum|DataDefinition
				</triplet>
				<triplet>
					DataDefinition->scaleOfMeasurement=DataType|CV
				</triplet>
			</triplets>
			<FormDesign>
				<Control Type="TextInput" Pos="1" Label="Name" Size="40" attributeAddress="]Datum|DataDefinition.name"/>
				<Control Type="TextInput" Pos="2" Label="Units" Size="40" attributeAddress="]Datum|DataDefinition.units"/>
				<Control Type="TextInput" Pos="3" Label="Value" Size="40" attributeAddress="]Datum|Datum.value"/>
				<Control Type="TextInput" Pos="4" Label="Error" Size="40" attributeAddress="]Datum|Datum.error"/>
				<Control Type="Lookup" Pos="5" Label="Type" Size="10"  lookupView="DataType" attributeAddress="]DataType|CV.indexString"/>
			</FormDesign>
		</PrimitiveDefinition>
		<PrimitiveDefinition Editable="0" Name="DataType" lookupView="DataType"/>
	</Primitives>
	<Index Format="$3$ : $2$ $1$ "/>
	<MachineIndex Format="$3$ : $2$ $1$"/>
	<IndexElements>
		<IndexElement Pos="1" attributeAddress="]Datum|DataDefinition.units"/>
		<IndexElement Pos="2" attributeAddress="]Datum|Datum.value"/>
		<IndexElement Pos="3" attributeAddress="]Datum|DataDefinition.name"/>
	</IndexElements>
	<Dependencies>
		<Dependency Name="Fragment"/>
	</Dependencies>
</ViewDefinition>

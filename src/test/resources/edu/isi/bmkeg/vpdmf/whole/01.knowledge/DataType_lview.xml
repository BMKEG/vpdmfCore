<ViewDefinition Name="DataType" Type="Lookup" Editable="0">
	<Primitives>		
		<PrimitiveDefinition Name="DataType" primaryFlag="TRUE">
			<Classes>
				<Class Name="CV" primaryFlag="TRUE"/>
			</Classes>
			<Conditions>
				<Condition>
					|CV.context='DataDefinition.scaleOfMeasurement'
				</Condition>
			</Conditions>
			<FormDesign>
				<Control Type="TextInput" Pos="1" Label="Name" Size="20" attributeAddress="]DataType|CV.name"/>
				<Control Type="TextInput" Pos="2" Label="Description" Size="20" attributeAddress="]DataType|CV.description"/>
			</FormDesign>			
		</PrimitiveDefinition>
	</Primitives>
	<Index Format="$1$: $2$."/>
	<IndexElements>
		<IndexElement Pos="1" attributeAddress="]DataType|CV.name"/>
		<IndexElement Pos="2" attributeAddress="]DataType|CV.description"/>
	</IndexElements>	
</ViewDefinition>		
<ViewDefinition Name="UptakeProperties" Type="Lookup">
	<Primitives>
		<PrimitiveDefinition Name="UptakeProperties" primaryFlag="TRUE">
			<Classes>
				<Class Name="LookupDatum" primaryFlag="TRUE"/>
				<Class Name="DataDefinition"/>
				<Class Name="CV"/>
			</Classes>
			<Conditions>
				<Condition>
					|LookupDatum.max='3'
				</Condition>
				<Condition>
					|DataDefinition.name='Strength'
				</Condition>
				<Condition>
					|DataDefinition.units='-'
				</Condition>
				<Condition>
					|DataDefinition.siConversionFactor='0'
				</Condition>
				<Condition>
					|CV.name='ordinal'
				</Condition>
			</Conditions>
			<triplets>
				<triplet>
					LookupDatum->def=UptakeProperties|DataDefinition
				</triplet>
				<triplet>
					DataDefinition->scaleOfMeasurement=UptakeProperties|CV
				</triplet>
			</triplets>
		</PrimitiveDefinition>
	</Primitives>
	<Index Format="hide[UptakeProperties]$1$ ($2$/$3$)"/>
	<MachineIndex Format="hide[UptakeProperties]$1$ ($2$/$3$)"/>
	<IndexElements>
		<IndexElement Pos="1" attributeAddress="]UptakeProperties|LookupDatum.value"/>
		<IndexElement Pos="2" attributeAddress="]UptakeProperties|LookupDatum.rank"/>
		<IndexElement Pos="3" attributeAddress="]UptakeProperties|LookupDatum.max"/>
	</IndexElements>
</ViewDefinition>
